
<% include ./partials/header %>

<% blogs.forEach(function(blog){ %>
    <div > 
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" style="width:30%">
        <span><%= blog.created %></span>
        <p><%= blog.body.substring(0,100) %>.....<a href="/blogs/<%= blog._id %>">Read More</a></p>
       
    </div>
<% }) %>


<% include ./partials/footer %>