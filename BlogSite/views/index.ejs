
<% include ./partials/header %>



<% blogs.forEach(function(blog){ %>
    <div > 
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" style="width:30%">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
    </div>
<% }) %>


<% include ./partials/footer %>